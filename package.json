    "github",
    "git"
  ],
  "bugs": {
    "url": "https://github.com/therebelrobot/mirror-repo/issues"
  },
  "homepage": "https://github.com/therebelrobot/mirror-repo#readme",
  "dependencies": {
    "breeze": "^1.1.7",
    "cli": "^0.11.0",
    "dotenv": "^1.2.0",
    "github": "^0.2.4",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "chai": "^3.3.0",
