  "main": "mirror.js",
  "bin": {
    "mirror": "bin/cli"
  },
  "scripts": {
    "pretest": "standard",
    "test": "istanbul cover _mocha --"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/therebelrobot/mirror-repo.git"
  },
